<!DOCTYPE html>
<html>
	<head>
		<title></title>
		<link rel="stylesheet" type="text/css" href="/assets/styles.css">

		<script type="text/javascript" src="js/webixadmin.js"></script>
		<link rel="stylesheet" type="text/css" href="css/webixadmin.css">
		<script src="js/socket.io.dev.js"></script>
	</head>
	<body>
		<h1 class='header'>
			<a href="/admin" style="float:left;">
				<div></div>
				DashBoard

			</a>
			<div id='logout' align='right'></div>
		
		</h1>
		<div class='mainarea'>
			<ul class='left'>
				{{#each menu}}
				<li><span>{{value}}</span><ul>
						{{#each data}}
						<li class='{{css}}'><a href='{{href}}'>{{value}}</a></li>
						{{/each}}
					</ul></li>
				{{/each}}
			</ul>
			<content class="right">{{{body}}}</content>
		</div>
		<script>
			  webix.ui({
         	view:'button',
				container:'logout',
				label:'logout',
				width:'150',
				click:"location.href ='admin';"
                });
			webix.ui({
			
			})
		</script>
	</body>
</html>